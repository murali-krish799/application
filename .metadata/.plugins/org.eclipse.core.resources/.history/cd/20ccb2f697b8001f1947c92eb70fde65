package com.krishsolutions.mutable;

/**we are demonstrate here mutable concept
 * of two ways
 *  those are
 *  StringBuffer version of 1.0
 *  StringBuilder version of 1.5
 * 
 */
public class MutableDemo {
	public static void main(String[] args) {
		StringBuffer sb= new StringBuffer("babu");
		System.out.println(sb.hashCode());
		
		
		
	}

}
